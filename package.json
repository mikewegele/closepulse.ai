{
  "name": "closepulse.ai",
  "version": "1.0.0",
  "private": true,
  "author": "closepulse.ai Team",
  "license": "MIT",
  "scripts": {
    "dev": "npm run frontend",
    "frontend": "cd pip_frontend && npm run dev",
    "frontend:build": "cd pip_frontend && npm run build",
    "frontend:preview": "cd pip_frontend && npm run preview",
    "backend:docker": "docker compose up api proxy",
    "backend:docker:stop": "docker compose down",
    "tunnel": "docker run -it --rm --network host cloudflare/cloudflared:latest tunnel --url http://localhost:80",
    "app": "concurrently -n fe,tunnel -c yellow,blue \"npm run frontend\" \"npm run tunnel\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
